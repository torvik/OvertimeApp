
<div class="container"> 
    
<h2>Time Tracker</h2>
<table class="table table-striped table-hover">
    <thead>
        <tr>
            <th>
                #
            </th>
            <th>
                Overtime Requested
            </th>
            <th>
                Date
            </th>
            <th>
                User
            </th>
            <th>
                Rationale
            </th>
            <th>
                Status
            </th>
            <th>
            </th>
            <th>
            </th>
        </tr>
    </thead>
    <tbody id="posts">
     <%= render @posts %> 
    </tbody>
</table>

<div id="paginator">
<%= paginate @posts, remote: true  %>
</div>

</div>